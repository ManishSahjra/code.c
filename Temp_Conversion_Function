#include <stdio.h>
float Temperature(float celsius);

int main() {
    float x;
    
    printf("Enter the temperature in Celsius: ");
    scanf("%f", &x); 
    
    float answer = Temperature(x);
    printf("The temperature in Fahrenheit is: %f\n", answer);

    return 0;
}

float Temperature(float celsius) {
    float answer = celsius * (9.0/5.0) + 32;
    return answer;
}
