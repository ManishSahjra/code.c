#include<stdio.h>
int sum(int a,int b);
int main(){
    int a,b;
    printf("Enter Your First number : ");
    scanf("%d",&a);
    printf("Enter Your Second number : ");
    scanf("%d",&b);

    int s = sum(a,b);
    printf("The sum is %d",s);
    return 0;

}
int sum (int x,int y){
    return x+y ;
}
