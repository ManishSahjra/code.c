#include <stdio.h>

int main() {
    int a = 5, b = 10;

    printf("Before swapping: a = %d, b = %d\n", a, b);

    a = a + b; // a now becomes 15 (5 + 10)
    b = a - b; // b now becomes 5 (15 - 10)
    a = a - b; // a now becomes 10 (15 - 5)

    printf("After swapping: a = %d, b = %d\n", a, b);

    return 0;
}
