#include<stdio.h>
int main(){
    int matrix[5][5] = {
        {4,7,2,8,4},
        {8,0,4,2,1},
        {9,4,1,8,0},
        {8,0,1,3,5},
        {9,1,3,1,8}
    };
    printf("The Diagonal are : \n ");
    for (int i = 0 ; i < 5; i++){
        for ( int j = 0 ; j < 5 ; j++){
            if (i == j){
                printf("%d %d",i,j);
            }
            else{
                printf("0 ");
            }
        }
        printf("\n");
    }

    return 0;
}
